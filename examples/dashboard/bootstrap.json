{
	"service": { "name": "devops-mcp", "version": "0.3.0" },
	"endpoint": "http://127.0.0.1:4318",
	"protocol": "http",
	"tracesUrl": "http://127.0.0.1:4318/v1/traces",
	"metricsUrl": "http://127.0.0.1:4318/v1/metrics",
	"logs": {
		"messageKey": "msg",
		"sink": "stderr",
		"filePath": "~/Library/Application Support/devops.mcp/logs/server.ndjson"
	},
	"redact": {
		"censor": "[REDACTED]",
		"paths": ["*.token", "*.password", "OPENAI_API_KEY"]
	}
}
